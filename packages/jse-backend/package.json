{
  "name": "jse-server",
  "version": "1.0.0",
  "description": "All jobs that has been scraped will be sent to this server",
  "main": "build/index.js",
  "scripts": {
    "dev": "nodemon index.ts",
    "migrate": "npx knex migrate:latest --knexfile database/knexfile.ts",
    "new_migration": "npx knex migrate:make init --migrations-directory database/migrations",
    "lint": "eslint .",
    "lintFix": "eslint . --fix",
    "prettier": "npx prettier --write .",
    "build": "rm -rf build && webpack",
    "start": "nodemon build/index.js"
  },
  "author": "Sayyid Muhammad A",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cheerio": "^1.0.0-rc.12",
    "cjs-loader": "^0.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint-config-airbnb-base": "^15.0.0",
    "express": "^4.18.2",
    "jse-jobs-scraper": "^1.7.4",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "ts-loader": "^9.4.2",
    "webpack": "^5.75.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
